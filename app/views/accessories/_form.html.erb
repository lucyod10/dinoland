<%= form_for @accessory, :html => {:multipart => true} do |f| %>
<%= f.label :name, :autofocus => true, :required => true %>
<%= f.text_field :name %>

<%= f.label :image, :required => true %>
<%= f.cl_image_upload :image %>

<%= f.label :cost, :required => true %>
<%= f.number_field :cost %>

<%= f.submit %>
<% end %>
